<view>
  <image style='width:100%;height:400rpx;' src='./images/bg_information.png'></image>
  <image style='width:200rpx;height:200rpx;position:absolute;top:100rpx;left:300rpx;;' src='./images/head_default.png'></image>
  <view class="table">
    <block wx:for="{{listData}}" wx:key="{{code}}">
      <view class="tr">
        <view class="td left">{{item.code}}</view>
        <view class="td right">{{item.text}}</view>
      </view>
    </block>
  </view>
</view>